@model IEnumerable<Visitas.Models.Visitass>

<div class="table-responsive">
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fe_Registro)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fe_Ingreso)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fe_Salida)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.No_Lugar_Reunion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VisitanteId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TrabajadorId)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Convert.ToString(string.Format("{0:dd/MM/yyyy}", item.Fe_Registro))
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Fe_Ingreso)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Fe_Salida)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.No_Lugar_Reunion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.VisitanteId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TrabajadorId)
                </td>
                <td>
                    <a href="#" onclick="visitas.getModal('@Url.Action("Edit2", new { id = item.Id})'); return false;"
                       data-toggle="modal" data-target="#modal-container" class="btn btn-info btn-sm">
                        <span class="glyphicon glyphicon-star"></span>
                    </a>
                    <a href="#" onclick="visitas.getModal('@Url.Action("Edit", new { id = item.Id})'); return false;"
                       data-toggle="modal" data-target="#modal-container" class="btn btn-warning btn-sm">
                        <span class="glyphicon glyphicon-edit"></span> 
                    </a>
                    <a href="#" onclick="visitas.getModal('@Url.Action("Details", new { id = item.Id})'); return false;"
                       data-toggle="modal" data-target="#modal-container" class="btn btn-primary btn-sm">
                        <span class="glyphicon glyphicon-search"></span> 
                    </a>
                    <a href="#" onclick="visitas.getModal('@Url.Action("Delete", new { id = item.Id})'); return false;"
                       data-toggle="modal" data-target="#modal-container" class="btn btn-danger btn-sm">
                        <span class="glyphicon glyphicon-trash"></span> 
                    </a>
                </td>
            </tr>
        }
    </table>
</div>