<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
	</configSections>
	<log4net>
		<appender name="RollingFile" type="log4net.Appender.RollingFileAppender">
			<file value="C:\logVisitas\VisitasWebApi.log" />
			<appendToFile value="true" />
			<maximumFileSize value="100000KB" />
			<maxSizeRollBackups value="10" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date %-level %logger - %message%newline" />
			</layout>
		</appender>
		<root>
			<level value="DEBUG" />
			<appender-ref ref="RollingFile" />
			<appender-ref ref="aiApprender" />
		</root>
	</log4net>
	<connectionStrings>
		<add name="VisitasConnection" connectionString="Server=LAPTOP-54DRFCGQ\SQLEXPRESS; Database=Visitas;Trusted_Connection=True;MultipleActiveResultSets=True;" providerName="System.Data.SqlClient" />
	</connectionStrings>
